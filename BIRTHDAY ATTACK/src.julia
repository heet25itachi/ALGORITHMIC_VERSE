const R = 65536

function birthday_attack()
    table = Dict{Int, Int}()
    trials = 0
    while true
        x = rand(Int)
        h = x % R
        trials += 1
        if haskey(table, h) && table[h] != x
            println("Collision found after $trials trials: x_i = $(table[h]), x_j = $x, hash = $h")
            break
        end
        table[h] = x
    end
end

birthday_attack()
